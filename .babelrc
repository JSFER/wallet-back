{
    "presets": ["es2015", "stage-0", "react"],
    "plugins": [
        ["transform-decorators-legacy"],
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": true,
                "regenerator": true
            }
        ],
        [
            "import",
            {
                "libraryName": "antd",
                "style": "css"
            }
        ]
    ]
}
